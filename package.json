{
  "name": "todoes",
  "version": "0.0.1",
  "description": "To Do + ES",
  "author": "azder <azhder@gmail.com>",
  "license": "MIT",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "config": {
    "port": 8888,
    "bin": "./node_modules/.bin",
    "transpile": "./node_modules/.bin/babel-node --presets=es2015",
    "devnode": "./node_modules/.bin/nodemon --ext js,xml,json",
    "load": "--load ./examples/load-eg-01.xml"
  },
  "scripts": {
    "dev": "${npm_package_config_devnode} --exec ${npm_package_config_transpile} -- ./${npm_package_main} ${npm_package_config_load}",
    "start": "node ${npm_package_config_transpile}  -- ./${npm_package_main}",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "lokijs": "^1.3.16",
    "minimist": "^1.2.0",
    "nodemon": "^1.9.1",
    "restify": "^4.0.4",
    "xml2js": "^0.4.16"
  }
}
